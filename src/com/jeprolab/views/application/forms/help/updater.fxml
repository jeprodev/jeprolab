<?import com.jeprolab.assets.extend.controls.JeproFormPanel?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<JeproFormPanel xmlns:fx="http://javafx.com/fxml" fx:id="jeproLabCheckForUpdateWrapper" fx:controller="com.jeprolab.controllers.JeproLabCheckForUpdateController" >
    <Pane fx:id="jeproLabCheckForUpdateContainerWrapper">
        <GridPane fx:id="jeproLabCheckForUpdateLayout">
            <Label fx:id="jeproLabInstalledVersionLabel" GridPane.columnIndex="0" GridPane.rowIndex="0" styleClass="input-label" />
            <Label fx:id="jeproLabInstalledVersion" GridPane.columnIndex="1" GridPane.rowIndex="0" />
            <Label fx:id="jeproLabAvailableVersionLabel" GridPane.columnIndex="2" GridPane.rowIndex="0" styleClass="input-label" />
            <Label fx:id="jeproLabAvailableVersion" GridPane.columnIndex="3" GridPane.rowIndex="0" />
            <Label fx:id="jeproLabNewFeaturesLabel" GridPane.columnIndex="0" GridPane.rowIndex="1"  styleClass="input-label" />
            <Text fx:id="jeproLabNewFeatures" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.columnSpan="3" />
            <!-- Label fx:id="jeproLabInstalledVersion" GridPane.columnIndex="" GridPane.rowIndex="" / -->
            <HBox GridPane.columnIndex="0" GridPane.rowIndex="4" GridPane.columnSpan="4" alignment="CENTER" spacing="20" >
                <Button fx:id="jeproLabIgnoreUpdateButton" />
                <Button fx:id="jeproLabUpdateButton" />
            </HBox>
        </GridPane>
    </Pane>
</JeproFormPanel>