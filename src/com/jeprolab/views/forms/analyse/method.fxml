<?import com.jeprolab.assets.extend.controls.JeproFormPanel?>
<?import com.jeprolab.assets.extend.controls.JeproFormPanelContainer?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import com.jeprolab.assets.extend.controls.JeproFormPanelTitle?>
<?import javafx.scene.layout.HBox?>
<JeproFormPanel xmlns:fx="http://javafx.com/fxml" fx:id="jeproLabMethodFormWrapper" fx:controller="com.jeprolab.controllers.JeproLabAnalyzeAddMethodController" >
    <JeproFormPanelTitle fx:id="jeproLabMethodFormTitleWrapper"/>
    <JeproFormPanelContainer fx:id="jeproLabMethodFormContentWrapper" >
        <GridPane fx:id="jeproLabMethodFormLayout">
            <Label GridPane.columnIndex="0" GridPane.rowIndex="0" fx:id="jeproLabAnalyzeMethodDesignationLabel" styleClass="input-label" />
            <TextField GridPane.columnIndex="1" GridPane.rowIndex="0" fx:id="jeproLabAnalyzeMethodDesignation"  />
            <Label GridPane.columnIndex="0" GridPane.rowIndex="1" fx:id="jeproLabAnalyzeMethodCodeLabel" styleClass="input-label" />
            <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" fx:id="jeproLabAnalyzeMethodCode"  />
            <Label GridPane.columnIndex="0" GridPane.rowIndex="2" fx:id="jeproLabAnalyzeMethodThresholdLabel" styleClass="input-label" />
            <HBox GridPane.columnIndex="1" GridPane.rowIndex="2" spacing="10" fx:id="jeproLabAnalyzeMethodThresholdWrapper" >
                <TextField fx:id="jeproLabAnalyzeMethodThreshold" />
                <Label fx:id="jeproLabAnalyzeMethodThresholdUnitLabel" styleClass="input-label" />
                <TextField  fx:id="jeproLabAnalyzeMethodThresholdUnit" />
            </HBox>
        </GridPane>
    </JeproFormPanelContainer>
</JeproFormPanel>